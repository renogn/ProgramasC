#include <stdio.h>

int main()
{
    int n;

    printf("Digite um valor inteiro: \n");
    scanf("%d", n);

    for(int i=2; i<n; i++)
    {
        int teste;
        teste = n%i;

        while(teste != 0)
        {
            teste = n%i;
        }

        if(teste == 0)
        {
            printf("%d e primo.\n", n);
            break;
        }
    }

    printf("%d nao e primo.\n", n);

    return 0;
}
