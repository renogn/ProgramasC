#include <stdio.h>

int main()
{
    printf("Digite um numero inteiro:\n");
    
    int n;
    scanf("%d", &n);

    for(int i=2; i<n; i++)
    {
        if(n%i == 0)
        {
            printf("%d nao e um numero primo\n", n);
            return 0;
        }
    }

    printf("%d e um numero primo\n", n);

    return 0;
}
